<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <!-- 填充的颜色 -->
            <solid android:color="#447744" />
            <stroke android:color="#773377" />
            <!-- 设置按钮的四个角为弧形 -->
            <corners android:radius="@dimen/round_button_padding" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <!-- 填充的颜色 -->
            <solid android:color="#337733" />
            <stroke android:color="#113311" />
            <!-- 设置按钮的四个角为弧形 -->
            <corners android:radius="@dimen/round_button_padding" />
        </shape>
    </item>

</selector>